[flake8]
max-line-length = 88
extend-ignore = E203, W503
exclude =
    .git,
    __pycache__,
    .venv,
    venv,
    env,
    .env,
    node_modules,
    staticfiles,
    media,
    migrations,
    build,
    dist,
    *.egg-info,
    .tox
per-file-ignores =
    __init__.py:F401
    config/settings.py:E501
    manage.py:E402

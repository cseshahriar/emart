# .flake8
[flake8]
max-line-length = 88
extend-ignore = E203, W503
ignore = E501
exclude =
    .git,
    __pycache__,
    .venv,
    venv,
    env,
    .env,
    migrations,
    static,
    media,
    *.egg-info,
    *.sqlite3,
    .tox,
    .pytest_cache,
    .coverage,
    htmlcov
per-file-ignores =
    __init__.py:F401
max-complexity = 10
